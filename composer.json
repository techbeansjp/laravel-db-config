{
  "name": "techbeansjp/laravel-database-configuration",
  "description": "Get the database configuration connected in laravel",
  "type": "library",
  "license": "MIT",
  "minimum-stability": "stable",
  "authors": [
    {
      "name": "adachi",
      "email": "tatsuru.adachi@techbeans.co.jp"
    }
  ],
  "autoload": {
    "psr-4": {
      "Techbeansjp\\LaravelDatabaseConfiguration\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Techbeansjp\\LaravelDatabaseConfiguration\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^8.0",
    "doctrine/dbal": "^3.2.7",
    "illuminate/database": "^v8.0.0",
    "illuminate/support": "^8.0.0",
    "illuminate/contracts": "^8.0.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Techbeans\\LaravelDatabaseConfiguration\\app\\Providers\\DbConfigServiceProvider"
      ],
      "aliases": {
        "DbConfig": "Techbeansjp\\LaravelDatabaseConfiguration\\app\\Facades\\DbConfig"
      },
      "commands": [
        "Techbeansjp\\LaravelDatabaseConfiguration\\app\\Console\\Commands\\DbConfigCommand"
      ]
    }
  }
}