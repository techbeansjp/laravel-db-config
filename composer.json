{
  "name": "techbeansjp/laravel-database-configuration",
  "description": "Get the database configuration connected in laravel",
  "type": "library",
  "license": "MIT",
  "minimum-stability": "dev",
  "authors": [
    {
      "name": "adachi",
      "email": "tatsuru.adachi@techbeans.co.jp"
    }
  ],
  "autoload": {
    "psr-4": {
      "Techbeansjp\\": "vendor/techbeansjp/laravel-database-configuration/src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Techbeansjp\\Tests\\": "vendor/techbeansjp/laravel-database-configuration/tests"
    }
  },
  "require": {
    "php": "^8.0",
    "doctrine/dbal": "^3.2.7",
    "illuminate/database": "^7.0|^8.0|^9.0|^10.0",
    "illuminate/support": "^7.0|^8.0|^9.0|^10.0",
    "illuminate/contracts": "^7.0|^8.0|^9.0|^10.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Techbeansjp\\LaravelDatabaseConfiguration\\App\\Providers\\DbConfigServiceProvider"
      ],
      "aliases": {
        "DbConfig": "Techbeansjp\\LaravelDatabaseConfiguration\\App\\Facades\\DbConfigFacade"
      }
    }
  }
}